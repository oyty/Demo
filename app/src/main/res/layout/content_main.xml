<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.circle.stu.ui.activity.MainActivity"
    tools:showIn="@layout/app_bar_main">

    <com.circle.stu.widget.imageslider.SliderLayout
        android:id="@+id/mSliderLayout"
        android:layout_width="match_parent"
        android:layout_height="260dp"
        app:auto_cycle="true"
        app:indicator_visibility="visible"
        app:pager_animation="Default"
        app:pager_animation_span="1100" />

    <com.circle.stu.widget.imageslider.Indicators.CircleIndicator
        android:id="@+id/mCircleIndicator"
        android:layout_width="match_parent"
        android:layout_marginTop="230dp"
        android:layout_height="30dp"  />


</RelativeLayout>
